
@model List<UAT_Migration>
<div class="card">
    <div class="card-header">
        Migrations
    </div>
    <div class="card-body">
        <div class="col-md-12">
            <table class="table table-sm">
                <thead>
                    <tr>
                        <th>
                            Date
                        </th>
                        <th>
                            Application
                        </th>
                        <th>
                            State
                        </th>

                    </tr>
                </thead>
                <tbody>
                   @foreach(var item in Model) {
                       <tr>
                           <td>
                               @item.DateUATPackage.ToLongDateString();
                           </td>
                           <td>
                               @item.Application.Label
                           </td>
                           <td>
                               @if (string.IsNullOrEmpty(item.Error)) {
                                   @("Success")
                               } else {
                                   @item.Error
                               }

                           </td>
                       </tr>
                   }
                </tbody>
            </table>
        </div>
    </div>
</div>

